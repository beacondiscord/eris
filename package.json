{
  "name": "eris",
  "version": "0.16.0",
  "description": "A NodeJS Discord library",
  "main": "./index.js",
  "exports": {
    ".": [
      {
        "require": "./index.js",
        "import": "./esm.mjs"
      },
      "./index.js"
    ],
    "./": "./",
    "./esm": "./esm.mjs"
  },
  "typings": "./index.d.ts",
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
    "format:check": "prettier --check \"lib/**/*.js\"",
    "format:fix": "prettier --write \"lib/**/*.js\"",
    "esm": "gen-esm-wrapper index.js esm.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abalabahaha/eris.git"
  },
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "eris",
    "wrapper"
  ],
  "author": "abalabahaha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abalabahaha/eris/issues"
  },
  "homepage": "https://abal.moe/Eris/",
  "dependencies": {
    "@tbnritzdoge/events": "^1.0.5",
    "form-data": "^4.0.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/ws": "^8.2.0",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
